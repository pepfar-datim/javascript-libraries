    "module": "esnext",
    "moduleResolution": "Node",
    "sourceMap": true,
    "strict": false,
    "noImplicitAny": false,
    "outDir": "./jsbuild",
    "allowJs": true,
    "declaration": false
  },
  "exclude": [
    "node_modules",
    "test"
  ],
  "include": [
    "src",
    "lib"
  ]
}