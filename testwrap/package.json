{
  "name": "@pepfar-react-lib/testwrap",
  "version": "0.0.14",
  "description": "Intuitive wrapper for @testing-library",
  "main": "index.ts",
  "exports": {
    ".": {
      "import": "./index.ts"
    },
    "jsbuild": {
      "import": "./jsbuild/index.js"
    }
  },
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "build:js": "rm -rf jsbuild/* && tsc --outDir jsbuild"
  },
  "peerDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.1",
    "@types/react": "^18.0.21",
    "jest": "^29.1.2",
    "react": "^18.2.0"
  },
  "author": "Jakub Flaska",
  "license": "ISC",
  "private": false,
  "devDependencies": {
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6"
  }
}
